{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.name) }}</div>
    <div class="col-md-4">{{ form_row(form.projects) }}</div>
    <div class="col-md-4">{{ form_row(form.email) }}</div>

</div>
<div class="row">
    <div class="col-md-10 col-sm-10 col-xs-10">{{ form_row(form.countries) }}</div>
</div>
<button class="btn btn-primary">{{ button|default('Send') }}</button>
{{ form_end(form) }}